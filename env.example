# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT=gpt-4o
AZURE_OPENAI_API_VERSION=2024-02-01

# Deepgram Voice Processing
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Pinecone Vector Database
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-west1-gcp-free
PINECONE_INDEX_NAME=agentic-comms-memory

# Railway Database & Redis
DATABASE_URL=postgresql://user:password@localhost:5432/agentic_comms
REDIS_URL=redis://localhost:6379

# Authentication & Security
JWT_SECRET=your_jwt_secret_here_change_in_production
API_KEY=your_api_key_for_external_integrations

# External Integrations
EMAIL_PROVIDER=smtp
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@company.com
SMTP_PASSWORD=your_app_password

# Grafana Observability
GRAFANA_API_KEY=your_grafana_api_key_here
GRAFANA_ORG_ID=your_org_id

# Environment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# Scaling Configuration
MAX_CONCURRENT_AGENTS=1000
WORKER_POOL_SIZE=50
REDIS_STREAM_BATCH_SIZE=10

# Performance Targets
MAX_RESPONSE_TIME_MS=5000
MAX_TTS_LATENCY_MS=1000
TARGET_AUTO_RESOLUTION_RATE=0.8 